    "# first lets try and look at only loans with done status.\n",
    "loan_info = loan_info[loan_info['loan_status'].isin(['paid', 'defaulted', 'charged_off'])]\n",
    "npv_rois = npv_rois.loc[loan_info.index,:]"
   ]
