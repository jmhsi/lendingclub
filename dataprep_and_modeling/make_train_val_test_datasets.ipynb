   "source": [
    "# take last quarter of nearly done as test/valid, take all loans with end_d before first month of last quarter to train on\n",
    "cutoff_issue_d = nearly_done['issue_d'].max() + pd.DateOffset(months=-3)\n",
    "test_valid = nearly_done[nearly_done['issue_d'] >= cutoff_issue_d]\n",
    "train = nearly_done[nearly_done['end_d'] < cutoff_issue_d]"
   ]
